{
  "appsettings": {

    "IntegrationEnpoint": "https://v3.football.api-sports.io/fixtures",
    "IntegrationApiKey": "{Your API Key}",
    "IntegrationApiHost": "v3.football.api-sports.io"

  },
  "Logging": {
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Debug",
        "Microsoft*": "None",
        "Hangfire": "None"
      },
      "InstrumentationKey": ""
    },
    "LogLevel": {
      "Default": "Information",
      "Microsoft*": "None",
      "Hangfire": "None"
    }
  },
  "Caching": {
    "Type": "InMemory",
    "AbsoluteExpiration": "30",
    "SlidingExpiration": "20"


  },
  "EventBus": {
    "MessageBrokerService": "RabbitMQ",
    "RabbitMQConnectionString": "amqp://guest:guest@pocrabbitmq.poc:5672",
    "RabbitMQManagementUrl": "",
    "RabbitMQUserName": "guest",
    "RabbitMQPassword": "guest"


  },
  "ConnectionStrings": {
    "poc_entities": "Server={Your Server Name};Initial Catalog={database Name};Persist Security Info=False;User ID={User Id};Password={Password};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=True;Connection Timeout=30;"
  },
  "AllowedHosts": "*",
  "ApplicationInsights": {
    "InstrumentationKey": ""
  }
}
